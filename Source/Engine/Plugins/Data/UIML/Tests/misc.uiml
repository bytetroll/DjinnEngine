<style>
    .highlighted {
        color: rgb(200, 200, 200);
    }

    .ctrl {
        color: rgb(100, 100, 100);
    }

    .seperator {
        color: rgb(100, 100, 100);
    }

    menubar {
        using .ctrl;
        vertical-scaling: content;
        horizontal-scaling: fill;
        z-offset: 0;
        margin: 0px 0px;
    }

    menuitem {
        using .ctrl;
        vertical-scaling: fill;
        horizontal-scaling: content;
    }

    menuitem image:hover {
        using .highlighted;
    }

    menuitem image:pressed {
        using menuitem image:hover;
    }
</style>

